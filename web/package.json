{
  "name": "modelgate-web",
  "type": "module",
  "version": "1.3.13",
  "packageManager": "pnpm@10.11.0",
  "description": "A fresh and elegant admin template, based on Vue3、Vite3、TypeScript、NaiveUI and UnoCSS. 一个基于Vue3、Vite3、TypeScript、NaiveUI and UnoCSS的清新优雅的中后台模版。",
  "author": {
    "name": "modelgate",
    "email": "yearnfar@gmail.com",
    "url": "https://github.com/modelgate/modelgate"
  },
  "license": "MIT",
  "homepage": "https://github.com/modelgate/modelgate",
  "repository": {
    "url": "https://github.com/modelgate/modelgate.git"
  },
  "bugs": {
    "url": "https://github.com/modelgate/modelgate/issues"
  },
  "keywords": [
    "Vue3 admin ",
    "vue-admin-template",
    "Vite5",
    "TypeScript",
    "naive-ui",
    "naive-ui-admin",
    "ant-design-vue v4",
    "UnoCSS"
  ],
  "engines": {
    "node": ">=18.20.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "build": "vite build --mode prod",
    "build:test": "vite build --mode test",
    "cleanup": "sa cleanup",
    "commit": "sa git-commit",
    "commit:zh": "sa git-commit -l=zh-cn",
    "dev": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "gen-route": "sa gen-route",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "sa release",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
  "dependencies": {
    "@better-scroll/core": "2.5.1",
    "@connectrpc/connect": "^2.0.2",
    "@connectrpc/connect-web": "^2.0.2",
    "@iconify/vue": "5.0.0",
    "@sa/axios": "workspace:*",
    "@sa/color": "workspace:*",
    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/utils": "workspace:*",
    "@vueuse/core": "13.3.0",
    "clipboard": "2.0.11",
    "dayjs": "1.11.13",
    "defu": "6.1.4",
    "echarts": "5.6.0",
    "json5": "2.2.3",
    "markdown-it": "^14.1.0",
    "naive-ui": "2.41.0",
    "nprogress": "0.2.0",
    "pinia": "3.0.2",
    "tailwind-merge": "3.3.0",
    "vue": "3.5.15",
    "vue-draggable-plus": "0.6.0",
    "vue-i18n": "11.1.5",
    "vue-router": "4.5.1",
    "wangeditor": "4.7.15"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.54.0",
    "@bufbuild/protobuf": "^2.5.1",
    "@elegant-router/vue": "0.3.8",
    "@iconify/json": "2.2.342",
    "@iconify/utils": "^2.3.0",
    "@sa/scripts": "workspace:*",
    "@sa/uno-preset": "workspace:*",
    "@soybeanjs/eslint-config": "1.6.1",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "22.15.23",
    "@types/nprogress": "0.2.3",
    "@unocss/eslint-config": "66.1.2",
    "@unocss/preset-icons": "66.1.2",
    "@unocss/preset-uno": "66.1.2",
    "@unocss/transformer-directives": "66.1.2",
    "@unocss/transformer-variant-group": "66.1.2",
    "@unocss/vite": "66.1.2",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitejs/plugin-vue-jsx": "4.2.0",
    "consola": "3.4.2",
    "eslint": "9.27.0",
    "eslint-plugin-vue": "10.1.0",
    "kolorist": "1.8.0",
    "lint-staged": "16.1.0",
    "sass": "1.89.0",
    "simple-git-hooks": "2.13.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "unplugin-icons": "22.1.0",
    "unplugin-vue-components": "28.7.0",
    "vite": "6.3.5",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "7.7.6",
    "vue-eslint-parser": "10.1.3",
    "vue-tsc": "2.2.10"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm sa git-commit-verify",
    "pre-commit": "scripts/pre-commit.sh"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "website": "https://www.modelgate.io"
}